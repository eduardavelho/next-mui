{
  "name": "@egvelho/next-mui",
  "version": "1.3.0",
  "description": "Material UI components for Next.js",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build tsconfig.build.json && cp -rf src/typography/*.woff* typography",
    "clean-project": "node scripts/clean-project",
    "typecheck": "tsc",
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "serve-storybook": "serve storybook-static -l 6006",
    "deploy-storybook": "gh-pages -d storybook-static",
    "copy-typography": "node ./scripts/copy-typography.js ./public ./typography"
  },
  "bin": {
    "copy-typography": "./scripts/copy-typography.js"
  },
  "author": "egvelho",
  "license": "GPL-3.0",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "react-infinite-scroll-component": "^6.1.0",
    "react-masonry-css": "^1.0.16",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/cli": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "gh-pages": "^3.2.3",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "next": "=11.1.0",
    "react": "=17.0.2",
    "react-dom": "=17.0.2"
  }
}
