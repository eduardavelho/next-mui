{
  "name": "@egvelho/next-mui",
  "version": "1.8.0",
  "description": "Material UI components for Next.js",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build tsconfig.build.json && rm -rf typography && cp -rf src/typography typography",
    "clean-project": "node scripts/clean-project",
    "typecheck": "tsc",
    "start-storybook": "start-storybook -p 6006 -s ./src/typography",
    "build-storybook": "build-storybook",
    "serve-storybook": "serve storybook-static -l 6006",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "author": "egvelho",
  "license": "GPL-3.0",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@types/react-input-mask": "^3.0.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-input-mask": "^2.0.4",
    "react-masonry-css": "^1.0.16",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/cli": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "gh-pages": "^3.2.3",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "next": "=11.1.0",
    "react": "=17.0.2",
    "react-dom": "=17.0.2"
  }
}
